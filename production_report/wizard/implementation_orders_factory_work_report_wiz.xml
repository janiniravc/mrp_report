<?xml version="1.0" ?>
<openerp>
	<data>

		<record model="ir.ui.view" id="view_implementation_orders_factory_work_report_wiz">
			<field name="name">implementation.orders.factory.work.report.wiz</field>
			<field name="model">implementation.orders.factory.work.report.wiz</field>
			<field name="arch" type="xml">
			 	<form string="product status Report" version="7.0">
                    <group col="4" colspan="4">
                        <field name="start_date" required="1"/>
                        <field name="end_date" required="1"/>
                    </group>
                    <footer>
                        <button name="print_report" string="Print" icon="gtk-print" type="object" class="oe_highlight"/>
                        <button string="Cancel" class="oe_link" special="cancel"/>
                    </footer>
				</form>
			</field>
		</record>
		
		<record model="ir.actions.act_window" id="action_implementation_orders_factory_work_report_wiz">
			<field name="name">Implementation Orders of the Factory Work </field>
			<field name="res_model">implementation.orders.factory.work.report.wiz</field>
			<field name="view_type">form</field>
			<field name="view_mode">form</field>
			<field name="target">new</field>
		</record>

		<menuitem name="Implementation Orders of the Factory Work" 
				  parent="menu_production_report" 
				  id="menu_implementation_orders_factory_work_report_wiz" 
				  action="action_implementation_orders_factory_work_report_wiz"/>
	</data>
</openerp>
