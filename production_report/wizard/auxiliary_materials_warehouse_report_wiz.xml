<?xml version="1.0" ?>
<openerp>
	<data>

		<record model="ir.ui.view" id="view_auxiliary_materials_warehouse_report_wiz">
			<field name="name">auxiliary.materials.warehouse.report.wiz</field>
			<field name="model">auxiliary.materials.warehouse.report.wiz</field>
			<field name="arch" type="xml">
			 	<form string="Auxiliary Materials in Warehouse Report" version="7.0">
                    <group col="4" colspan="4">
                        <field name="product_ids" required="1"/>
                    </group>
					<footer>
                        <button name="print_report" string="Print" icon="gtk-print" type="object" class="oe_highlight"/>
                        <button string="Cancel" class="oe_link" special="cancel"/>
                    </footer>
				</form>
			</field>
		</record>
		
		<record model="ir.actions.act_window" id="action_auxiliary_materials_warehouse_report_wiz">
			<field name="name">Auxiliary Materials in Warehouse Report</field>
			<field name="res_model">auxiliary.materials.warehouse.report.wiz</field>
			<field name="view_type">form</field>
			<field name="view_mode">form</field>
			<field name="target">new</field>
		</record>

		<menuitem name="Auxiliary Materials in Warehouse Report" 
				  parent="menu_production_report" 
				  id="menu_auxiliary_materials_warehouse_report_wiz" 
				  action="action_auxiliary_materials_warehouse_report_wiz"/>
	</data>
</openerp>
