<?xml version="1.0" ?>
<openerp>
	<data>

		<record model="ir.ui.view" id="view_number_produced_units_report_wiz">
			<field name="name">number.produced.units.report.wiz</field>
			<field name="model">number.produced.units.report.wiz</field>
			<field name="arch" type="xml">
			 	<form string="ECN Report" version="7.0">
                    <group col="4" colspan="4">
<!--                         <field name="year_id" required="1"/> -->
<!--                         <label string=""/> -->
                    </group>
					<footer>
                        <button name="print_report" string="Print" icon="gtk-print" type="object" class="oe_highlight"/>
                        <button string="Cancel" class="oe_link" special="cancel"/>
                    </footer>
				</form>
			</field>
		</record>
		
		<record model="ir.actions.act_window" id="action_number_produced_units_report_wiz">
			<field name="name">Number Of Produced Units</field>
			<field name="res_model">number.produced.units.report.wiz</field>
			<field name="view_type">form</field>
			<field name="view_mode">form</field>
			<field name="target">new</field>
		</record>

		<menuitem name="Number Of Produced Units" 
				  parent="menu_production_report" 
				  id="menu_number_produced_units_report_wiz" 
				  action="action_number_produced_units_report_wiz"/>
	</data>
</openerp>
