<?xml version="1.0" ?>
<openerp>
	<data>

		<record model="ir.ui.view" id="view_work_of_contractor_report_wiz">
			<field name="name">work.of.contractor.report.wiz</field>
			<field name="model">work.of.contractor.report.wiz</field>
			<field name="arch" type="xml">
			 	<form string="Work of Contractor Report" version="7.0">
                    <group col="4" colspan="4">
                        <field name="start_date" required="1"/>
                        <field name="end_date" required="1"/>
                    </group>
					<footer>
                        <button name="print_report" string="Print" icon="gtk-print" type="object" class="oe_highlight"/>
                        <button string="Cancel" class="oe_link" special="cancel"/>
                    </footer>
				</form>
			</field>
		</record>
		
		<record model="ir.actions.act_window" id="action_work_of_contractor_report_wiz">
			<field name="name">Work of Contractor Report</field>
			<field name="res_model">work.of.contractor.report.wiz</field>
			<field name="view_type">form</field>
			<field name="view_mode">form</field>
			<field name="target">new</field>
		</record>

		<menuitem name="Work of Contractor Report" 
				  parent="menu_report_maintenance" 
				  id="menu_work_of_contractor_report_wiz" 
				  action="action_work_of_contractor_report_wiz"/>
	</data>
</openerp>
